{
	"compileOnSave": true,
	"compilerOptions": {
		"outDir": "./out-tsc",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es6",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2015",
			"dom"
		]
	},
	"exclude": [
		"node_modules"
	]
}
